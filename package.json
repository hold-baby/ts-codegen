{
  "name": "@ts-tool/ts-codegen",
  "version": "0.7.8",
  "description": "Typescript code generator, which is able to convert swagger to ts code.",
  "main": "lib/index.js",
  "module": "module/index.js",
  "types": "src/index.ts",
  "bin": {
    "ts-codegen": "bin/cli.js"
  },
  "files": [
    "dist",
    "bin",
    "lib",
    "module",
    "src",
    "README.md"
  ],
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/codegen.ts",
    "test": "tsc && jest",
    "build": "rm -rf dist && tsc",
    "release": "rm -rf lib/ && rm -rf module/ && rollup -c rollup.config.ts",
    "prepare": "npm run build && npm run release && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reeli/ts-codegen.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reeli/ts-codegen/issues"
  },
  "homepage": "https://github.com/reeli/ts-codegen#readme",
  "devDependencies": {
    "@babel/core": "7.9.x",
    "@babel/preset-env": "7.9.x",
    "@babel/preset-typescript": "7.9.x",
    "@types/axios": "0.14.x",
    "@types/jest": "25.2.x",
    "@types/lodash": "4.14.x",
    "@types/node": "14.0.x",
    "@types/prettier": "2.0.x",
    "@types/rollup": "0.54.x",
    "axios": "0.19.x",
    "babel-core": "6.26.x",
    "babel-jest": "26.0.x",
    "babel-plugin-pure-calls-annotation": "0.3.x",
    "commander": "5.1.0",
    "jest": "26.0.x",
    "lodash": "4.17.x",
    "rollup": "2.10.x",
    "rollup-plugin-babel": "4.4.x",
    "rollup-plugin-typescript": "1.0.x",
    "ts-jest": "25.5.x",
    "ts-node": "8.10.x",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.x",
    "@types/swagger-schema-official": "2.0.x"
  },
  "dependencies": {
    "prettier": "^2.0.x",
    "axios": "^0.19.x",
    "lodash": "^4.17.15",
    "tslib": "^2.0.x"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>"
    ],
    "coverageDirectory": "coverage",
    "testRegex": ".*/__tests__/.+\\.(generator|test|spec)\\.(ts|tsx)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/__types__/**",
      "!src/__tests__/**"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest",
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
